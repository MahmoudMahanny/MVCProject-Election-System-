@model ElectionProgram.ShowModel.CandidatesIDVoter
@{
    ViewBag.Title = "ShowCandidate";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}
@section header{
    <div>show candidates</div>
}
<link href="~/Content/ShowcandidateStyle.css" rel="stylesheet" />
<table id="mytable">
    <tr class="Header" id="myHeader">
        <th>Candidate Name</th>
        <th>Number of Votes</th>
        <th>Candidate Image</th>
        </tr>
    
        @{
            foreach (var item in Model.canList)
            {
                <tr>

                    <td>@item.Name </td>
                    <td>@item.NoOfVotes </td>
                    <td><img src="@item.ImagePath" width="50px" height="50px"/> </td>
                    }

                </tr>

                }
            }



    </table>
    
       
           
        <div class="btn btn-info" style="text-align:center;color:white">
            @Html.ActionLink("Goto MYPage", "MYPage", new { id = Model.VoterID })
        </div>
            
  
